<!DOCTYPE html>
<html>
<head>
    <title></title>
    <link href="~/css/logintemplate.css" rel="stylesheet" />
    <link href="~/css/bootstrap.css" rel="stylesheet" />
    <script src="~/js/app.js" defer></script>
</head>
<body>
    <div class="wrapper" id="app">
        <div class="imagebackground">
            <div class="container-fluid px-1 px-md-5 px-lg-1 px-xl-5 py-5 mx-auto">
                <div class="card card0 border-0">
                    <div class="row d-flex">
                        <div class="col-lg-6">
                            <img style="margin-left:400px" src="~/Image/navylogo2.png" class="logo">
                        </div>
                        <div class="col-lg-6">
                            <div class="card2 card border-0 px-4 py-5">
                                <h3 style="color:brown">@TempData["commandLoginMessage"]</h3>
                                <div class="row mb-4 px-3">
                                    <h3 class="mb-0 mr-4 mt-2">Admin Login Here</h3>

                                </div>
                                <div class="row px-3 mb-4">
                                    <div class="line"></div>
                                    <div class="line"></div>
                                </div>
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                    <div class="bg-blue py-4">
                        <div class="row px-3">
                            <small class="ml-4 ml-sm-5 mb-2">
                                Copyright &copy; @DateTime.Now.Year<a style="color:yellowgreen" href="https://hicadsystemsltd.com/" _blank>Hicad Systems LTD</a> All rights
                                reserved.
                            </small>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="~/js/loginjs.js"></script>
    <link href="~/css/font-awsome.css" rel="stylesheet" />
    <script src="~/js/jQuery.js"></script>
</body>
</html>
